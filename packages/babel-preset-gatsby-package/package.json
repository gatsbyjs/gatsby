    "babel-plugin-dynamic-import-node": "^2.3.3",
    "core-js": "^3.10.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.15",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
