  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "bluebird": "^3.7.2",
    "chokidar": "^3.4.3",
    "fs-exists-cached": "^1.0.0",
    "gatsby-core-utils": "^1.5.0-next.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "micromatch": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
