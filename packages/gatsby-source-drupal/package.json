    "gatsby-source-filesystem": "^2.1.26",
    "lodash": "^4.17.15",
    "tiny-async-pool": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "babel-preset-gatsby-package": "^0.2.5",
