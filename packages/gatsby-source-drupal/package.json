  "version": "4.3.0-next.1",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "gatsby-source-filesystem": "^3.3.0-next.1",
    "lodash": "^4.17.21",
    "tiny-async-pool": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
