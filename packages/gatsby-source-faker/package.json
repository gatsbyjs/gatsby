{
  "name": "gatsby-source-faker",
  "description": "A gatsby plugin to get fake data for testing",
  "version": "3.12.0-next.2",
  "author": "Pavithra Kodmad<pavithra.kodmad@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "faker": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "babel-preset-gatsby-package": "^1.12.0-next.2",
    "cross-env": "^7.0.3"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-faker#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-source-faker"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
