{
  "name": "gatsby-source-wordpress-experimental",
  "description": "Source data from WPGraphQL in an efficient and scalable way.",
  "version": "0.0.12",
  "author": "Tyler Barnes <tylerdbarnes@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@rematch/core": "^1.3.0",
    "@rematch/immer": "^1.2.0",
    "async-retry": "^1.3.1",
    "atob": "^2.1.2",
    "axios": "^0.19.0",
    "axios-rate-limit": "^1.2.1",
    "babel-preset-gatsby": "^0.2.29",
    "clipboardy": "^2.1.0",
    "dumper.js": "^1.3.1",
    "execall": "^2.0.0",
    "graphql-prettier": "^1.0.6",
    "graphql-query-compress": "^1.2.2",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "babel-jest": "^25.1.0",
    "babel-plugin-import-globals": "^2.0.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-source-map-support": "^2.1.1",
    "babel-preset-gatsby-package": "^0.2.9",
    "cross-env": "^5.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "react-test-renderer": "^16.12.0",
    "tree-kill": "^1.2.2",
    "wait-on": "^4.0.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-source-wordpress#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-source-wordpress"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__ --verbose",
    "setup-test-runtime": "cd test-runtime && rm -rf ./node_modules/gatsby-source-wordpress-experimental && yarn install --check-files",
    "clean-test-runtime": "cd test-runtime && gatsby clean",
    "develop-test-runtime": "cd test-runtime && yarn develop",
    "build-test-runtime": "cd test-runtime && yarn build",
    "ci": "yarn prepare && yarn setup-test-runtime && yarn test",
    "ci-develop": "yarn prepare && yarn test-develop",
    "test": "yarn clean-test-runtime && WPGRAPHQL_INCREMENT=false jest && WPGQL_INCREMENT=true jest",
    "test-develop": "jest --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
