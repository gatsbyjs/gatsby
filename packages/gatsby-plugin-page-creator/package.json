{
  "name": "gatsby-plugin-page-creator",
  "version": "4.4.0-next.0",
  "description": "Gatsby plugin that automatically creates pages from React components in specified directories",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\" --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "contributors": [
    "Steven Natera <tektekpush@gmail.com> (https://twitter.com/stevennatera)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-plugin-page-creator"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-plugin-page-creator#readme",
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@babel/traverse": "^7.15.4",
    "@sindresorhus/slugify": "^1.1.2",
    "chokidar": "^3.5.2",
    "fs-exists-cached": "^1.0.0",
    "gatsby-core-utils": "^3.4.0-next.0",
    "gatsby-page-utils": "^2.4.0-next.0",
    "gatsby-plugin-utils": "^2.4.0-next.0",
    "gatsby-telemetry": "^3.4.0-next.0",
    "globby": "^11.0.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.4.0-next.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "gatsby": "^4.0.0-next"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
