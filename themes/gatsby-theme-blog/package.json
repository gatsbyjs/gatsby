{
  "name": "gatsby-theme-blog",
  "description": "Gatsby Blog Theme",
  "version": "0.0.6",
  "author": "Chris Biscardi <chris@christopherbiscardi.com>",
  "bugs": {
    "url": "https://github.com/christopherbiscardi/gatsby-theme-examples/issues"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "debug": "^4.1.0",
